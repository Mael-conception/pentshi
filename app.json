{
  "expo": {
    "name": "pentshi",
    "slug": "pentshi",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vanotis720.pentshi"
    },
    "android": {
      "package": "com.vanotis720.pentshi",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "react-native-document-scanner-plugin",
        {
          "cameraPermission": "Nous avons besoin d'un accès à la caméra pour que vous puissiez numériser des documents"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6fcad6a1-c4f9-468c-a1b0-9b79f02f4cff"
      }
    }
  }
}
